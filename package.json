{
  "name": "pucktracker-client",
  "version": "0.0.0",
  "description": "Frontend for puck tracking system",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --hot --inline --host 0.0.0.0 --port 5900",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/helper.js --recursive",
    "test:watch": "NODE_ENV=test npm run test -- --watch",
    "coverage": "NODE_ENV=test istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --require ./test/helper.js --recursive",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobbieClarken/pucktracker-client.git"
  },
  "author": "Robbie Clarken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobbieClarken/pucktracker-client/issues"
  },
  "homepage": "https://github.com/RobbieClarken/pucktracker-client#readme",
  "dependencies": {
    "bootstrap": "3.3.5",
    "classnames": "2.2.0",
    "history": "1.13.0",
    "immutable": "3.7.5",
    "react": "0.14.2",
    "react-bootstrap": "0.27.3",
    "react-dom": "0.14.2",
    "react-immutable-proptypes": "1.5.0",
    "react-pure-render": "1.0.2",
    "react-redux": "4.0.0",
    "react-router": "1.0.0",
    "react-router-bootstrap": "0.19.3",
    "react-wysiwyg": "2.2.0",
    "redux": "3.0.4",
    "socket.io-client": "1.3.7"
  },
  "devDependencies": {
    "babel-core": "5.8.24",
    "babel-loader": "5.3.2",
    "babel-plugin-react-transform": "1.1.1",
    "chai": "3.4.1",
    "chai-immutable": "1.5.2",
    "coveralls": "2.11.4",
    "css-loader": "0.22.0",
    "file-loader": "0.8.4",
    "istanbul": "0.4.0",
    "jsdom": "3.1.2",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "mocha": "2.3.3",
    "react-addons-test-utils": "0.14.2",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.1.1",
